Object.filter = function(obj, predicate, singleton) {
    var result = {}, key;
    // ---------------^---- as noted by @CMS,
    //      always declare variables with the "var" keyword

    for (key in obj) {
        if (obj.hasOwnProperty(key) && predicate(obj[key])) {
            result[key] = obj[key];
        }
    }

    if (singleton) {
        var key = Object.keys(result)[0];
        result = result[key];
    }

    return result;
}
